<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    tools:context=".getinvolved.Indivisual">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"

        >

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:text="@string/raw_string"
        android:textSize="@dimen/normal_text_12sp"

        ></androidx.appcompat.widget.AppCompatTextView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:layout_height="130dp"
            >





            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/bhd_500"

                android:layout_width="0dp"
                android:layout_height="0dp"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"

                app:layout_constraintHeight_percent="0.95"
                app:layout_constraintWidth_percent="0.4"

                android:src="@drawable/ic_launcher_background"

                ></de.hdodenhof.circleimageview.CircleImageView>





        </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="130dp"
        >





        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/bhd_1000"
            android:layout_width="0dp"
            android:layout_height="0dp"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/general_sponsor"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"

            app:layout_constraintHeight_percent="0.95"
            app:layout_constraintWidth_percent="0.4"

            android:src="@drawable/ic_launcher_background"

            ></de.hdodenhof.circleimageview.CircleImageView>



        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/bhd_2000"
            android:layout_width="0dp"
            android:layout_height="0dp"

            app:layout_constraintStart_toEndOf="@+id/bhd_1000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"

            app:layout_constraintHeight_percent="0.95"
            app:layout_constraintWidth_percent="0.4"

            android:src="@drawable/ic_launcher_background"

            ></de.hdodenhof.circleimageview.CircleImageView>

    </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:layout_height="130dp"
            >





            <de.hdodenhof.circleimageview.CircleImageView

                android:id="@+id/additional_option"
                android:layout_width="0dp"
                android:layout_height="0dp"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"

                app:layout_constraintHeight_percent="0.95"
                app:layout_constraintWidth_percent="0.4"

                android:src="@drawable/ic_launcher_background"

                ></de.hdodenhof.circleimageview.CircleImageView>





        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.core.widget.NestedScrollView>